{
  "extends": "airbnb",
  "env": {
    "es6": true,
    "browser": true
  },
  "rules": {
    "max-statements": [
      "error",
      8
    ],
    "max-depth": [
      "error",
      4
    ],
    "max-params": [
      "error",
      3
    ],
    "react/destructuring-assignment": [
      "error",
      "always"
    ]
  }
}